<h1>User index</h1>
<table>
    <tr>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Email</th>
    </tr>
    <% @users.each do |user| %>
    <tr>
        <th><%= user.first_name %></th>
        <th><%= user.last_name %></th>
        <th><%= user.email %></th>
    </tr>
    <% end %>
</table>
