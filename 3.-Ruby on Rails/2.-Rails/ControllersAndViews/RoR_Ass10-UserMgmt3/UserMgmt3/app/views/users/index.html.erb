<h1>User index</h1>
<table>
    <tr>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Email</th>
        <th>Show</th>
        <th>Edit</th>
        <th>Destroy</th>
    </tr>
    <% @users.each do |user| %>
    <% user_id = user.id %>
    <tr>
        <th><%= user.first_name %></th>
        <th><%= user.last_name %></th>
        <th><%= user.email %></th>
        <th><a href="users/<%= user_id %>">Show</a></th>
        <th><a href="users/<%= user_id %>/edit">Edit</a></th>
        <th><a href="/users/<%= user_id %>/delete">Delete</a></th>
    </tr>
    <% end %>
</table>

<a href="/users/new">New User</a>
